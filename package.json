{
  "name": "gaql-builder",
  "version": "0.1.0",
  "description": "A TypeScript library for building Google Ads Query Language (GAQL) queries programmatically",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist coverage",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build && npm run test && npm run lint && npm run typecheck"
  },
  "keywords": [
    "google-ads",
    "gaql",
    "query-builder",
    "typescript",
    "google-ads-api"
  ],
  "author": "Traffic by Intent",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trafficbyintent/gaql-builder.git"
  },
  "bugs": {
    "url": "https://github.com/trafficbyintent/gaql-builder/issues"
  },
  "homepage": "https://github.com/trafficbyintent/gaql-builder#readme",
  "devDependencies": {
    "@types/node": "^20.19.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.0.0",
    "google-ads-api": "^20.0.1",
    "long": "^5.3.2",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
