{
  "name": "@trafficbyintent/gaql-builder",
  "version": "1.0.0",
  "description": "A TypeScript library for building Google Ads Query Language (GAQL) queries programmatically",
  "sideEffects": false,
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "txi-protocols": {
    "version-update-method": "manual-edit-only",
    "forbidden-npm-commands": [
      "version",
      "publish"
    ],
    "forbidden-git-commands": [
      "commit",
      "push",
      "tag",
      "merge"
    ],
    "test-coverage-required": 100,
    "commit-permission": "explicit-only"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist coverage",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build && npm run test && npm run lint && npm run typecheck",
    "version": "npm test && git add -A",
    "postversion": "git push && git push --tags",
    "version:local": "npm test && git add -A && echo 'Version updated locally. Run \"git push && git push --tags\" when ready.'"
  },
  "keywords": [
    "google-ads",
    "gaql",
    "query-builder",
    "typescript",
    "google-ads-api"
  ],
  "author": "Traffic by Intent",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/txi-dev/gaql-builder.git"
  },
  "bugs": {
    "url": "https://github.com/txi-dev/gaql-builder/issues"
  },
  "homepage": "https://github.com/txi-dev/gaql-builder#readme",
  "devDependencies": {
    "@trafficbyintent/style-guide": "^1.2.1",
    "@types/deep-eql": "^4.0.2",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.1.0",
    "@types/yargs-parser": "^21.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
